public class Main {  
     public static void main(String[] args) {  
         java.util.Scanner sc = new java.util.Scanner(System.in);  
         int n = sc.nextInt();  
         int all=1;  
         for(int i=1;i<=n;i++) {  
             all=all*i;  
         }  
         System.out.println(all);  
     }  
}  
