public class Main{		
	public static void main(String[] args) {
		java.util.Scanner sc = new java.util.Scanner(System.in);
		 int n = sc.nextInt();
		 int[] each = new int[n];
		 for(int i=0;i<n;i++) each[i] = sc.nextInt();
		 int mini=each[0];
		 
		 for(int i=1;i<n;i++) {
			 if(each[i]<mini)mini=each[i];
		 }
		 System.out.printf("%d\n",mini);
	}
}
